<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com https://mysterycode.de/XSD/templateListener.xsd">
	<import>
		<templatelistener name="todoNotification">
			<environment>user</environment>
			<templatename>userNotice</templatename>
			<eventname>userNotice</eventname>
			<templatecode><![CDATA[{include file='__toDoWarning'}]]></templatecode>
		</templatelistener>
		<templatelistener name="todoCopyright">
			<environment>user</environment>
			<templatename>footer</templatename>
			<eventname>copyright</eventname>
			<templatecode><![CDATA[{if $templateName == 'toDo' || $templateName == 'toDoAdd' || $templateName == 'toDoCategory' || $templateName == 'toDoList' || $templateName == 'myToDos' || $templateName == 'toDoTrash' || $templateName == 'toDoArchive'}<address class="copyright marginTop">{lang}wcf.toDo.copyright{/lang}</address>{/if}]]></templatecode>
		</templatelistener>
	</import>
</data>
